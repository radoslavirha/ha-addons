{
  "name": "laskakit-data-feeder",
  "version": "0.0.1",
  "description": "Data Feeder for LaskaKit interactive map of Czech republic",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "swc src --out-dir dist -s --strip-leading-paths",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "sync:version": "sync-json -p version -s package.json config.json",
    "start": "nodemon src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node --import @swc-node/register/esm-register dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@radoslavirha/config-typescript": "0.0.3",
    "@radoslavirha/tsed-configuration": "0.0.2",
    "@radoslavirha/tsed-platform": "0.0.5",
    "@swc-node/register": " ^1.10.9",
    "@swc/cli": "^0.5.2",
    "@swc/helpers": "^0.5.15",
    "@tsed/ajv": "^8.3.2",
    "@tsed/core": "^8.3.2",
    "@tsed/di": "^8.3.2",
    "@tsed/engines": "^8.3.2",
    "@tsed/exceptions": "^8.3.2",
    "@tsed/json-mapper": "^8.3.2",
    "@tsed/logger": "^7.0.1",
    "@tsed/platform-cache": "^8.3.2",
    "@tsed/platform-exceptions": "^8.3.2",
    "@tsed/platform-express": "^8.3.2",
    "@tsed/platform-http": "^8.3.2",
    "@tsed/platform-log-request": "^8.3.2",
    "@tsed/platform-middlewares": "^8.3.2",
    "@tsed/platform-params": "^8.3.2",
    "@tsed/platform-response-filter": "^8.3.2",
    "@tsed/schema": "^8.3.2",
    "ajv": "^8.17.1",
    "body-parser": "^1.20.3",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "method-override": "^3.0.0",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@radoslavirha/config-eslint": "0.0.5",
    "@radoslavirha/config-vitest": "0.0.4",
    "@swc/core": "^1.10.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/method-override": "^3.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^2.1.8",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0",
    "sync-json": "^1.0.2",
    "tslib": "^2.8.1",
    "vitest": "^2.1.8"
  },
  "tsed": {
    "convention": "conv_default",
    "architecture": "arc_default",
    "packageManager": "pnpm",
    "runtime": "node"
  },
  "engines": {
    "node": ">= 20.18.1",
    "pnpm": ">= 9.14.4"
  }
}